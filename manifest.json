{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icons/icon-24.png",
      "default_popup": "popup.html",
      "default_title": "Chromarks Plus"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content.css" ],
      "js": [ "jquery.min.js", "content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval'  https://ssl.google-analytics.com; object-src 'self'",
   "description": "Chrookmarks Plus - Popup bookmarks manager. Easy and fast management of your bookmarks",
   "icons": {
      "128": "icons/icon-128.png",
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "40",
   "name": "Chromarks Plus - Chrome Bookmarks Manager",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "bookmarks", "chrome://favicon/", "storage" ],
   "short_name": "Chromarks",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}
